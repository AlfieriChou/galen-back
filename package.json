{
  "name": "sequelize_swagger",
  "version": "1.0.0",
  "description": "Base on sequelize to generate swagger.json",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "release": "NODE_ENV=release nodemon server.js",
    "db:migrate": "node ./migrations/migration.js",
    "db:migrate:prod": "node ./migrations/migration.js prod",
    "db:migrate:release": "node ./migrations/migration.js release",
    "db:drop": "node ./migrations/drop.js",
    "db:drop:prod": "node ./migrations/drop.js prod",
    "db:drop: release": "node ./migrations/drop.js release",
    "lint": "eslint --ext .js ./ --fix",
    "test": "NODE_ENV=development nyc mocha --exit",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfieriChou/sequelize_swagger2.0.git"
  },
  "keywords": [
    "sequelize",
    "swagger"
  ],
  "author": "AlfieriChou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlfieriChou/sequelize_swagger2.0/issues"
  },
  "homepage": "https://github.com/AlfieriChou/sequelize_swagger2.0#readme",
  "dependencies": {
    "app-root-path": "2.2.1",
    "bluebird": "^3.5.5",
    "body-parser": "1.19.0",
    "consolidate": "^0.15.1",
    "dir_filenames": "^1.0.6",
    "express": "4.17.1",
    "jsonschema": "^1.2.4",
    "lodash": "4.17.15",
    "morgan": "^1.9.1",
    "mustache": "3.1.0",
    "mysql2": "1.7.0",
    "sequelize": "5.19.0",
    "walk-sync": "2.0.2",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "codecov": "^3.1.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-standard": "^3.0.1",
    "husky": "^4.0.2",
    "lint-staged": "^9.2.5",
    "mocha": "^6.0.0",
    "nodemon": "^1.18.7",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "supertest": "^4.0.2"
  }
}
