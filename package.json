{
  "name": "sequelize_swagger",
  "version": "1.0.0",
  "description": "Base on sequelize to generate swagger.json",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "release": "NODE_ENV=release nodemon server.js",
    "db:migrate": "node ./migrations/migration.js",
    "db:migrate:prod": "node ./migrations/migration.js prod",
    "db:migrate:release": "node ./migrations/migration.js release",
    "db:drop": "node ./migrations/drop.js",
    "db:drop:prod": "node ./migrations/drop.js prod",
    "db:drop: release": "node ./migrations/drop.js release",
    "lint": "standard --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfieriChou/sequelize_swagger2.0.git"
  },
  "keywords": [
    "sequelize",
    "swagger"
  ],
  "author": "AlfieriChou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlfieriChou/sequelize_swagger2.0/issues"
  },
  "homepage": "https://github.com/AlfieriChou/sequelize_swagger2.0#readme",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "consolidate": "^0.15.1",
    "dir_filenames": "^1.0.6",
    "easy-monitor": "^2.2.5",
    "express": "^4.16.3",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "mustache": "^2.3.2",
    "mysql2": "^1.6.1",
    "sequelize": "^4.38.0",
    "walk-sync": "^0.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.4",
    "standard": "^12.0.1"
  }
}
