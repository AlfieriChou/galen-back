{
  "name": "galen",
  "version": "1.2.0",
  "description": "Base on sequelize to generate swagger.json",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "release": "standard-version",
    "migrate": "node ./migrations/migrate.js",
    "migrate:prod": "node ./migrations/migrate.js prod",
    "migrate:release": "node ./migrations/migrate.js release",
    "lint": "eslint --ext .js ./ --fix",
    "test": "NODE_ENV=development nyc mocha --exit",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlfieriChou/galen.git"
  },
  "keywords": [
    "sequelize",
    "swagger"
  ],
  "author": "AlfieriChou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlfieriChou/galen/issues"
  },
  "homepage": "https://github.com/AlfieriChou/galen#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "dir_filenames": "^1.0.6",
    "expire-store": "^1.0.2",
    "express": "4.17.1",
    "jsonschema": "^1.2.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "nunjucks": "^3.2.0",
    "sequelize": "^5.21.3",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "codecov": "^3.7.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-standard": "^3.0.1",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.3",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "should": "^13.2.3",
    "standard-version": "^7.1.0",
    "supertest": "^4.0.2"
  }
}
